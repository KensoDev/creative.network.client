{
  parser: "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/prefer-stateless-function": 0,
    "no-multi-spaces": 0,
    "react/jsx-curly-spacing": 0,
    "react/prop-types": 0,
    "react/jsx-no-bind": 0,
    "camelcase": 0,
    "default-case": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-new": 0,
    "prefer-template": 0,
    "quotes": 0,
    "new-cap": 0,
    "no-else-return": 0,
    "arrow-body-style": 0,
    "no-use-before-define": 0,
    "space-before-function-paren": [2, { "anonymous": "never", "named": "never" }],
    "max-len": [1, 120, 2, {ignoreComments: true}]
  }
}
